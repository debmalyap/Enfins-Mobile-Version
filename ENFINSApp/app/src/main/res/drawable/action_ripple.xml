    android:color="#7BB1FF">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="#154bad" />
            <corners
                android:bottomRightRadius="4dip"
                android:topRightRadius="4dp" />
        </shape>
    </item>
</ripple>