<?xml version="1.0" encoding="utf-8"?>

<GridLayout
    android:id="@+id/memberEligibilityCheckGridLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="10dp"
    android:layout_marginTop="0dp"
    android:background="@drawable/grid_list_bg"
    android:orientation="vertical"
    android:padding="10dp"
    android:layout_column="1"
    android:layout_row="5"
    android:numColumns="auto_fit"
    app:layout_constraintTop_toTopOf="parent"
    tools:layout_editor_absoluteX="4dp">

    <!--Application Number Layout-->
    <LinearLayout
        android:layout_row="0"
        android:layout_column="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textMemberEligibityCode"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/outline_poll_black_36dp"
            android:drawablePadding="5dp"
            android:fontFamily="sans-serif-medium"
            android:padding="5dp"
            android:textColor="#414141"
            android:textSize="16sp" />

    </LinearLayout>
    <!--End of Application Number-->

    <!--Member Name Layout-->
    <LinearLayout
        android:layout_row="1"
        android:layout_column="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">



        <TextView
            android:id="@+id/textMemberEligibityName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/outline_account_box_black_36dp"
            android:drawablePadding="5dp"
            android:fontFamily="sans-serif-medium"
            android:padding="5dp"
            android:textColor="#414141"
            android:textSize="16sp" />

    </LinearLayout>
    <!--End Of Member Name-->

    <!--Adhar Number Layout-->
    <LinearLayout
        android:layout_row="2"
        android:layout_column="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textMemberEligibityAadhar"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/aadhar"
            android:drawablePadding="5dp"
            android:fontFamily="sans-serif-medium"
            android:padding="5dp"
            android:textColor="#414141"
            android:textSize="16sp" />

    </LinearLayout>
    <!--End of Adhar Number-->

    <!--Collection Point Name Layout-->
    <LinearLayout
        android:layout_row="3"
        android:layout_column="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textMemberEligibityCollectionPoint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/outline_location_on_black_36dp"
            android:drawablePadding="5dp"
            android:fontFamily="sans-serif-medium"
            android:padding="5dp"
            android:textColor="#414141"
            android:textSize="16sp" />

    </LinearLayout>
    <!--End of Collection Point Name-->

    <!--Organization Layout-->
    <LinearLayout
        android:layout_row="4"
        android:layout_column="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textMemberEligibityOrganizationName"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/outline_account_balance_black_36dp"
            android:drawablePadding="5dp"
            android:fontFamily="sans-serif-medium"
            android:padding="5dp"
            android:textColor="#414141"
            android:textSize="16sp" />



    </LinearLayout>
    <!--End of Organization Name Layout-->

    <!--Loan Product Layout-->
    <LinearLayout
        android:layout_row="5"
        android:layout_column="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textMemberEligibityLoanProductName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/money"
            android:drawablePadding="5dp"
            android:fontFamily="sans-serif-medium"
            android:padding="5dp"
            android:textColor="#414141"
            android:textSize="16sp" />

    </LinearLayout>
    <!--End of Loan Product Layout-->

    <!--Date Layout-->
    <LinearLayout
        android:layout_row="6"
        android:layout_column="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textMemberEligibityCheckedOn"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/outline_event_black_36dp"
            android:drawablePadding="5dp"
            android:fontFamily="sans-serif-medium"
            android:padding="5dp"
            android:textColor="#414141"
            android:textSize="16sp" />



    </LinearLayout>
    <!--End of Date Layout-->

    <!--Applicant Name Layout-->
    <LinearLayout
        android:layout_row="7"
        android:layout_column="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textMemberEligibityCheckedBy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/outline_assignment_turned_in_black_36dp"
            android:drawablePadding="5dp"
            android:fontFamily="sans-serif-medium"
            android:padding="5dp"
            android:textColor="#414141"
            android:textSize="16sp" />

    </LinearLayout>
    <!--End of Applicant Name-->

    <!--Member Status Layout-->
    <LinearLayout
        android:layout_row="8"
        android:layout_column="0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textMemberEligibityStatus"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/outline_assignment_turned_in_black_36dp"
            android:drawablePadding="5dp"
            android:fontFamily="sans-serif-medium"
            android:padding="5dp"
            android:textColor="#414141"
            android:textSize="16sp" />

    </LinearLayout>
    <!--End of Member Status Layout-->

</GridLayout>